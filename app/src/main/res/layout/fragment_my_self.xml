    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:id="@+id/my_relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="22dp"
        android:paddingBottom="22dp"
        android:paddingTop="22dp"
        android:paddingRight="12dp">
        <ImageView
            android:id="@+id/my_fragment_image1"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:src="@drawable/icon_login"/>
        <TextView
            android:id="@+id/my_fragment_text1"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="25dp"
            android:layout_toRightOf="@+id/my_fragment_image1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17dp"
            android:textColor="#333"
            android:text="点击登录"/>
        <TextView
            android:id="@+id/my_fragment_text2"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="16dp"
            android:layout_below="@+id/my_fragment_text1"
            android:layout_toRightOf="@+id/my_fragment_image1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:text="百科新闻"/>
        <ImageView
            android:layout_alignParentRight="true"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:src="@drawable/arrow_to_press"
            android:layout_centerVertical="true"
            />

