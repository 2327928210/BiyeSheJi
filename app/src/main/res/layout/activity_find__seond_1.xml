<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_find__seond_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.zhangzhaoqiang.theheadlines.ui.Find_Second_Activity1">
    <RelativeLayout
        android:id="@+id/find_2ji1_relative_layout"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:padding="10dp"
        android:background="#ffd700">
        <ImageView
            android:id="@+id/find_2ji1_image_title"
            android:layout_marginLeft="10dp"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/top_back"
            android:layout_centerVertical="true"/>
        <TextView
            android:id="@+id/find_2ji1_text_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:textColor="#333"
            android:text="萌萌哒"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:layout_centerVertical="true"/>
       </RelativeLayout>
    <ListView
        android:layout_below="@+id/find_2ji1_relative_layout"
        android:id="@+id/find_2ji1_listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"/>
    <RelativeLayout
        android:id="@+id/find_2ji1_empty_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <ProgressBar
            android:id="@+id/find_2ji1_progressBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
</RelativeLayout>
